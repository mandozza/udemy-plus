{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "udemy-plus/fancy-header",
    "title": "Fancy Header",
    "description": "Adds a header with a hover effect.",
    "category": "text",
    "keywords": [
        "header",
        "underline",
        "hover"
    ],
    "version": "1.0.0",
    "textdomain": "udemy-plus",
    "editorScript": "file:./index.js",
    "attributes": {
      "content": {
        "type": "string",
        "source": "html",
        "selector": "h2"
      },
      "underline_color": {
        "type": "string",
        "default": "#f87171"
      }
    },
    "editorStyle": "",
    "style": "file:./index.css"
}
